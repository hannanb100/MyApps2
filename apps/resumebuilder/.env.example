# Resumebuilder App Environment Variables
# Copy this file to .env and customize the values

# Debug settings
RESUMEBUILDER_DEBUG=True

# Paths
RESUMEBUILDER_TEMPLATE_PATH=./templates
RESUMEBUILDER_OUTPUT_PATH=./output

# File settings
RESUMEBUILDER_MAX_FILE_SIZE=10485760  # 10MB

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
RESUME_API_KEY=your_resume_api_key_here

# Database
DATABASE_URL=sqlite:///./resumebuilder.db
